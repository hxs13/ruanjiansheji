;Demo mode definition
[Demo Mode]
enabled = 1               ;Set to 1 to enable demo mode, 0 to disable
select.enabled = 0        ;Set to 1 to display select screen, 0 to disable
vsscreen.enabled = 0      ;When you hit ESC can restart in VS screen when char select is disable(This Feature is not done yet)
title.waittime = 1500      ;Time to wait at title before starting demo mode
fight.endtime = 1500      ;Time to display the fight before returning to title
fight.playbgm = 1         ;Set to 1 to enable in-fight BGM, 0 to disable
fight.stopbgm = 0		  ;Set to 1 to stop title BGM (only if playbgm = 0)
fight.bars.display = 0    ;Set to 1 to display lifebar, 0 to disable
intro.waitcycles = 0      ;Cycles to wait before intro cutscene is played again
debuginfo = 1             ;Set to 0 to hide debugging info (debug mode only)

;-------------------------------------------------------------------
;Continue screen definition
[Continue Screen]
